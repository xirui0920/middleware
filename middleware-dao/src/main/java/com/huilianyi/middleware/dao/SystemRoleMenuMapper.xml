<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huilianyi.middleware.dao.SystemRoleMenuMapper">
    <resultMap id="SystemRoleMenuResultMap" type="com.huilianyi.middleware.po.SystemRoleMenu"/>
    <select id="selectMenuByRoleId" resultType="java.lang.String">
        SELECT MENU_ID FROM SYSTEM_ROLE_MENU WHERE ROLE_ID = #{roleId}
    </select>
</mapper>
